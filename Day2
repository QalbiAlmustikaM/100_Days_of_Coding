public class PrimitifPenjumlahan {
    public static void main(String[] args) {
        int angkaPertama = 5;
        int angkaKedua = 3;

        // Melakukan penjumlahan
        int hasil = angkaPertama + angkaKedua;

        // Menampilkan hasil
        System.out.println("Hasil penjumlahan: " + hasil);
    }
}
